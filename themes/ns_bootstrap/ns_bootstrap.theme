/**
 * Implement hook_preprocess
 *
 * Adds useful theme variables.
 */
function ns_bootstrap_preprocess(&$variables, $hook, $info) {
  if (isset($variables['directory']))
    $variables['theme_path'] = base_path() . $variables['directory'];
}